<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<navigation-rule>
		<display-name>services.xhtml</display-name>
		<from-view-id>/services.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{servicesBean.revoke(connectorWrapper)}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/services.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>awaiting_services.xhtml</display-name>
		<from-view-id>/awaiting_services.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{servicesBean.save(connectorWrapper)}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/awaiting_services.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>
